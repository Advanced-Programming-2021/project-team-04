<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="yugioh.view.DuelView"
          prefHeight="834.4" prefWidth="613.2" alignment="CENTER" hgap="10" vgap="10" styleClass="root">
   <VBox>
       <HBox alignment="CENTER">
           <Label alignment="CENTER" fx:id="toShow" styleClass="text" textAlignment="CENTER"/>
       </HBox>
       <HBox spacing="10" alignment="CENTER">
           <TextField alignment="CENTER" fx:id="textField" styleClass="text-field" onKeyPressed="#onEnter"/>
       </HBox>
       <HBox alignment="CENTER">
           <Button text="Start" styleClass="start"/>
           <Button text="Back" onMouseClicked="#backToFirstPage"/>
       </HBox>
   </VBox>

    <stylesheets>
        <URL value="@stylesheets/Coin.css"/>
    </stylesheets>
</GridPane>
