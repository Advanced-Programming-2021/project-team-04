<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="yugioh.view.ScoreboardView"
          prefHeight="834.4" prefWidth="613.2" alignment="CENTER" hgap="10" vgap="10" styleClass="root">
    <VBox fx:id="scoreboard" GridPane.columnIndex="0" alignment="CENTER_LEFT"/>
    <Button text="Main Menu" GridPane.columnIndex="1" onMouseClicked="#mainMenu"/>
    <stylesheets>
        <URL value="@stylesheets/ScoreboardView.css"/>
    </stylesheets>
</GridPane>
